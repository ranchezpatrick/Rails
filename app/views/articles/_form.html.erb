
<%= form_with model: @article do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <br/>

    <%= f.label :author %>
    <%= f.text_field :author %>

    <br/>

    <%= f.label :content %>
    <%= f.text_area :content %>

    <br/>

    <%= f.submit %>
<% end %>
<%= link_to articles_path do %> Back to index <%end%>